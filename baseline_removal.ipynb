      "source": [
        "def baseline_remove(XR):\n",
        "  XT = XR[:, 384:]\n",
        "  XB_ = (XR[:, :1*128] + XR[:, 1*128:2*128] + XR[:, 2*128:3*128])/3.0\n",
        "  for i in range(60):\n",
        "    XT[:, i*128:(i+1)*128] = XT[:, i*128:(i+1)*128] - XB_\n",
        "  return XT"
      ]
    }
  ]
}